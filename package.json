{
  "name": "market.js",
  "version": "0.0.0",
  "description": "library for interacting with MARKET Protocol",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "test": "node scripts/test.js --env=jsdom",
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./index.ts",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MARKETProtocol/MARKET.js.git"
  },
  "keywords": [
    "MARKET Protocol",
    "ethereum",
    "blockchain",
    "dApp",
    "trading",
    "derivatives",
    "decentralized",
    "decentralized exchange",
    "exchange"
  ],
  "author": "Phil Elsasser",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/MARKETProtocol/MARKET.js/issues"
  },
  "homepage": "https://marketprotocol.io",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.4",
    "jest": "^22.4.3",
    "nodemon": "^1.17.4",
    "ts-jest": "^22.4.5",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
